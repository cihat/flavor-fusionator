{
  "name": "flavor-fusionator-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node --swc' src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "dev:nodemon": "nodemon dist/index.js",
    "dev:concurrent": "concurrently \"npm run watch\" \"npm run dev:nodemon\"",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "pretty-quick -w . && eslint . --fix",
    "lint-staged": "lint-staged",
    "test:watch": "jest --testEnvironment=node --verbose --forceExit --watchAll --maxWorkers=1",
    "test": "jest",
    "test-coverage": " jest --collect-coverage --testEnvironment=node --forceExit --maxWorkers=1"
  },
  "lint-staged": {
    "*.{js, ts}": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.{json|css|md}": [
      "pretty-quick --staged"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^7.2.2",
    "mongoose-autopopulate": "^1.0.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/mongoose-autopopulate": "^0.10.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "concurrently": "^8.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^29.5.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^4.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
